<div>
  <h1>Projects</h1>
</div>

<table>
  <thead>
    <tr>
      <td>Name</td>
      <td>Status</td>
      <td>Actions</td>
    </tr>
  </thead>
  <tbody>
    <% @projects.each do |project| %>
      <tr>
        <td><%= project.name %></td>
        <td><%= project.status.humanize %></td>
        <td>
          <%= link_to 'show more...', project_path(project) %></br>
          <%= link_to 'history', project_history_path(project) %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
